{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "g++ build active file",
			"command": "/usr/bin/gcc",
			"args": [
				"-g",
				"${workspaceFolder}//*.c",
				"-o",
				"${workspaceFolder}/runapp"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "compiler: /usr/bin/gcc"
		}
	]
}